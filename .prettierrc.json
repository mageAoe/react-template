{
   "endOfLine": "auto",
   "printWidth": 100,
   "tabWidth": 2,
   "useTabs": false,                    
   "semi": false,                      
   "singleQuote": true,                
   "quoteProps": "as-needed",          
   "jsxSingleQuote": true,             
   "trailingComma": "none",            
   "bracketSpacing": true,             
   "jsxBracketSameLine": true,         
   "arrowParens": "avoid",             
   "vueIndentScriptAndStyle": false,   
   "embeddedLanguageFormatting": "off"
}
